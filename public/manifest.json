{
  "manifest_version": 3,
  "name": "Tailwind Decoder",
  "description": "Tailwind Decoder",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "./assets/icon16.png",
      "32": "./assets/icon32.png",
      "48": "./assets/icon48.png",
      "128": "./assets/icon128.png"
    }
  },
  "icons": {
    "16": "./assets/icon16.png",
    "32": "./assets/icon32.png",
    "48": "./assets/icon48.png",
    "128": "./assets/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://tailwindui.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://tailwindui.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "compiled.css"
      ],
      "matches": [
        "https://tailwindui.com/*"
      ]
    }
  ]
}